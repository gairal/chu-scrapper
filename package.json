{
  "name": "chu-scrapper",
  "scripts": {
    "build": "tsc",
    "debug": "npx nodemon",
    "deploy": "npx firebase deploy --only functions --project com-gairal-chools",
    "dev": "node --inspect -r ts-node/register ./src/fastify.ts",
    "format": "npx prettier --write",
    "lint": "npx eslint ./src/**/*.ts",
    "logs": "npx firebase functions:log",
    "serve": "npm run build && npx firebase serve --only functions --project com-gairal-chools",
    "shell": "npm run build && npx firebase functions:shell --project com-gairal-chools",
    "start": "npm run debug"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "date-fns": "^2.9.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "googleapis": "^47.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^13.5.2",
    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fastify": "^2.11.0",
    "firebase-tools": "^7.12.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "private": true,
  "engines": {
    "node": "12"
  }
}
