{
  "name": "chu-scrapper",
  "scripts": {
    "build": "tsc",
    "debug": "npx nodemon",
    "deploy": "npx firebase deploy --project com-gairal-chrapper",
    "dev": "node --inspect -r ts-node/register ./src/index-dev.ts",
    "format": "npx prettier --write",
    "lint": "npx eslint ./src/**/*.ts",
    "logs": "npx firebase functions:log",
    "serve": "npm run build && npx firebase serve --only functions --project com-gairal-chrapper",
    "shell": "npm run build && npx firebase functions:shell --project com-gairal-chrapper",
    "start": "npm run debug",
    "test": "NODE_ENV=test npx jest"
  },
  "main": "dist/index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.9.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-plugin-jest": "^23.20.0",
    "fastify": "^3.2.0",
    "firebase-tools": "^8.6.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-mock-axios": "^4.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "private": true,
  "engines": {
    "node": "12"
  }
}
